{
  "name": "postgresql-research",
  "version": "0.1.0",
  "description": "PostgreSQL (and Trinoâ†’PostgreSQL) benchmark: bonus_registry table variants, batch load, read/query benchmarks",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "compose:up": "docker compose -f compose/docker-compose.yml up -d",
    "compose:down": "docker compose -f compose/docker-compose.yml down",
    "compose:restart": "docker compose -f compose/docker-compose.yml restart",
    "compose:reset": "docker compose -f compose/docker-compose.yml down --volumes --remove-orphans",
    "setup:tables": "tsx scripts/setup-tables.ts",
    "drop:tables": "tsx scripts/drop-tables.ts",
    "bench:fill": "tsx scripts/bench-fill.ts",
    "bench:read": "tsx scripts/bench-read.ts",
    "bench:queries": "tsx scripts/bench-queries.ts",
    "bench:full": "tsx scripts/bench-full.ts",
    "api:server": "tsx src/api/server.ts",
    "k6:insert-one": "k6 run k6-scripts/insert-one.js",
    "k6:insert-one:docker": "node scripts/k6-run-docker.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "postgres": "^3.4.7",
    "trino-client": "^0.2.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
